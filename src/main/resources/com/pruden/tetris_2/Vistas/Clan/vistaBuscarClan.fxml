<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="556.0" prefWidth="534.0" styleClass="pane-1" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pruden.tetris_2.Controladores.Clan.ControladorBuscarClan">
    <stylesheets>
        <URL value="@../../styles/general.css" />
    </stylesheets>

    <TextField fx:id="barraBusqueda" layoutX="32.0" layoutY="83.0" onKeyReleased="#filtrarClanes" prefHeight="26.0" prefWidth="459.0" promptText="Buscar por nombre..." />


    <Label fx:id="nombre" alignment="CENTER" layoutY="24.0" prefHeight="41.0" prefWidth="534.0" text="Buscar clan" textAlignment="CENTER" textFill="#e9e8e8">
        <font>
            <Font name="Liberation Mono Bold" size="32.0" />
        </font>
    </Label>

    <ScrollPane fitToWidth="true" hbarPolicy="NEVER" layoutX="31.0" layoutY="126.0" prefHeight="361.0" prefWidth="472.0">
        <content>
            <GridPane fx:id="gridClanes" hgap="10" vgap="10" />
        </content>
    </ScrollPane>
    <Button layoutX="235.0" layoutY="505.0" mnemonicParsing="false" onAction="#volver" text="Vovler" />


</Pane>
